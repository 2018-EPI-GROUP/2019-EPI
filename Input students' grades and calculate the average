#include <stdio.h>

# define NUM 10

int main()
{
       int s[NUM];
       int i,sum=0;
       
       for(i=0;i<10;i++)
       {     
           printf("Input %i 's grades:",i+1);
           scanf("%d,&s[i]");
           sum+=s[i];
       }
   
       printf("Finish.The average is :%f.2\n",(double)sum/NUM);
     
       return 0;
}
