main()
{ 
  long x;
  int ge,shi,qian,wan;
  printf("\n:3456789");
  scanf("%ld",&x);
  if(x>=10000&&x<100000)
    {   
        wan=x/10000;
        qian=x%10000/1000;
        shi=x%100/10;
        ge=x%10;
     if(ge==wan&&shi==qian)
           printf("\n");
     else  printf("\n");
   	}
   	else
     	{ 
	       printf("抱歉 \n");
   	       exit(l);
    	}
}
#include<stdio.h>
main()
{
  int i=1;
  double fac=1;
  while(i<=100)
  {
   fac*=i;
   i++;
  }
  printf("1*2*3*...*100=%if",fac);
}
#include<stdio.h> 
main() 
{ 
   int i,n,x,sum;
   i=1;sum=0;
   printf("Input number n:");
   scanf("%d",&n);
   do
   { 
     scanf("%d",&x);
     sum=sum+x;
     i++;
   }while(i<=n);
   printf("sum is: %d\n",sum);
}
