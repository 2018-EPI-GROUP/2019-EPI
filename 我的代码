#include<stdio.h>
int main()
{
    int x=2,y=-1,z=2;
    if(x<y)
    { 
        if(y>0)
        z=0;
    }
    else z+=1;
    printf("z=%d\n",z) ;
}
int a=3,b=5;
int max(int x,int y) 
{  
    int c;
    c=x>y?x:y;
    return c;
}
main()
{   
    int a=8;
    printf("%d\n",max(a,b));
}
#include<stdio.h>
main()
{ 
  int x,y,z,t;
  printf("\n请输入三个整数（x,y,z): ");
  scanf("%d,%d,%d",&x,&y,&z);
  if (x>y)
  { 
    t=x;
    x=y;
    y=t;
  }
  if(x>z)
  { 
    t=z;
    z=x;
    x=t;
  }
  if(y>z)
  {
    t=y;
    y=z;
    z=t;
  }
  printf("%d,%d,%d\n",x,y,z);
}
#include "math.h"
main() 
{
	int i,j;
	char s[36][68];
	float x=0.0,h=3.14159265/18;
	for(i=0;i<36;i++)
	{ 
		s[i][0]='|' ;
		s[i][67]='\0';
		for(j=i;j<67;j++)
		   s[i] [j]=' ';	
	}
    s[22][0]='+';
    for(j=1;j<66;j++)
        s[22][j]='-';
    s[22][66]='>';
    s[23][66]='x';
    printf("exp(-x)*cos(x)\n  ^\n");
    for(j=0;j<64;j++)
    {
    	i=(int)(22.5-22*exp(-x/6.0)*cos(x));
    	s[i][j]='*';
    	x+=h;
	}
	for(i=0;i<36;i++)
	  printf("    %s\n",s[i]);
}
#include<stdio.h>
main()
{
int year,leap=0;
printf("\n(yyyy):2001");
scanf("%d",&year);
/*判断该年是否是闰年*/
if(year%4==0&&year%100!=0||year%400==0) leap=1;
if(leap)
   printf("%d\n",year);
else
   printf("%d\n",year);
}
#include<stdio.h>
main()
{
	int m;
	int i;
	int flag;
	printf("m");
	scanf("%d",&m);
	flag=1;
	for(i=2;i<m;i++)
	  if(m%i==0 )
	  {
	  	flag=0;
	  	i=m;
	  	
	  }
	  if(flag==1)
	  printf("%d\n;m");
	  else
	  printf("%d\n;m");
}
