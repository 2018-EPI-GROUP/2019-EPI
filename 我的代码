#include<stdio.h> 
main()
{ 
 float x;
float sum=0;
scanf("%f",&x);
while(x>0)
{ 
  sum+=x;
  scanf("%f",&x);
}
printf("和%6.2f",sum);
}
#define PI 3.14159
#include<stdio.h>
main()
{
  float r;
  float s, l;
  /*3*/
  printf("请输入圆的半径：");
  scanf("%f",&r);
  /*求面积s 周长l*/
  s=PI*r*r;
  l=2*PI*r;
  /*输出面积s 周长l*/
  printf("面积=%6.3f, 周长=%6.3f \n", s, l) ;
}
#include<stdio.h>
int main()
{
	int a,b,c;
	for(a=2;a<=100;a++)
	{
		for(b=2;b<a;b++)
		{
			if(a%b==0)
			break;
		}
		if(b==a)
		printf("%d\n",a);
	}
	return 0;
}
#include<stdio.h> 
main()
{
	char c,s[81];
	int i,n=0,k=0;
	gets(s);
	for(i=0;c=s[i];c++)
	  if(c==' ')k=0;
	  else if(k=0)
	  {
	 	n++;
	 	k=1;
	  }
	 printf("There are %d words in the line \n",n);
 } 
#include<stdio.h>
struct st_type
{
	char num[7];
	char name[20];
	char sex;
	int age;
	float score;
	char address[30];
};
void print(struct st_type s)
{
	printf("\n");
	printf("%9s%20s%4c%6d%8.1f\n",s.num,s.name,s.sex,s.age,s.score);
}
main()
{
	struct st_type s0;
	printf("_");
	gets(s0.name);
	printf("\n");
	scanf("%s %c %d %f",s0.num,&s0.sex,&s0.age,&s0.score);
	print(s0);
}
#include<stdio.h>
main()
{ 
  char ch;
  ch=getchar();
  ch=ch>='a'&&ch<='z'?ch-32:ch;
  putchar(ch);
}
main()
{
	int i,j,n,a[101];
	printf("\ninput number:\n");
	scanf("%d",&n);
	if(n<=0) return;
	else a[1]=n;
	i=1;
	scanf("%d",&n);
	while(n>0)
	{
		for(j=i;n>a[j] && j>0;j--)
		   a[j+1]=a[j];
	    a[++j]=n;
	    i++;
	    scanf("%d",&n);
	}
	for(j=i;j<=i;j++)
    	printf("%d",a[j]);
	printf("\n");
}
