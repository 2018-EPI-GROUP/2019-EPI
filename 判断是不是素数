//输入一个数，判断该数是不是素数
#include<stdio.h>
int isPrime(int x)
{
	int ret = 1;
	int i;
	if(x==1){
		ret=0;
	}
	for(i=2;i<x;i++){
		if (x%i==0){
			ret = 0;
			break;
		}
	}
	return ret;
}

int main(void)
{
	int x;
	scanf("%d",&x);
	if(isPrime(x)){
		printf("%d是素数",x);
	} else {
		printf("%d不是素数",x);
	}
	return 0;
}
