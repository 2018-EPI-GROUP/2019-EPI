#include<stdio.h>//str->字符串    cmp  比较
int my_strcmp(char ch1[], char ch2[])
{
	int i = 0;
	while (ch1[i] == ch2[i])
	{
		//是否到字符串结尾
		if (ch1[i] == '\0')
		{
			return 0;

		}
		i++;
	}
	return ch1[i] > ch2[2] ? 1 : -1;
}
int main()
{
	char ch1[] = "hello";
	char ch2[] = "hallo";
	int value = my_strcmp(ch1, ch2);
	if (value == 0)
	{
		printf("相等");
	}

	return 0;
}
