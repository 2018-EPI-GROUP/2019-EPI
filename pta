#include <stdio.h>
#include <stdlib.h>

int main()
{
    int year,mon;
    scanf("%d %d",&year,&mon);
        switch(mon)
    {
    case 2:
        if((!(year%400))||(!year%4&&year%100))
            printf("29");
        else
            printf("28");
        break;
    case 1:
    case 3:
    case 5:
    case 7:
    case 8:
    case 10:
    case 12:
       {
        printf("31");
        break;
       }
    case 4:
    case 6:
    case 9:
    case 11:
        {
         printf("30");
         break;
        }

    }
    return 0;
}

#icnlude<stdio.h>
#include<stdlib.h>
int main()
{
    int a,b,d;
    char c;
    scanf("%d%c%d",&a,&c,&b);
    switch (c )
    {
    case '+':
        d=a+b;
        printf("%d",d);
        break;
    case '-':
        d=a-b;
        printf("%d",d);
        break;
    case '*':
        d=a*b;
        printf("%d",d);
        break;
    case '/':
        d=a*b;
        printf("%d",d);
        break;
    case '%':
        d=a%b;
        printf("%d",d);
        break;
    default:
        printf("ERROR");

    }
    return 0;
}

