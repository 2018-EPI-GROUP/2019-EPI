#include <stdio.h>
/**百钱买百鸡，类似1，2，5凑100银币的问题/*
int main()
{
    inti,j//公鸡，母鸡个数
    for(i=0,i<=20,i++);
    {
        for(i=0,j<33,j++)//母鸡
        {
            printf("公鸡%d,母鸡%d,小鸡%d\n,i,j,100-i-j");
        }
    }
}
//1,2,5凑100银币问题
int main()
{
    int count = o;//情况数
    inti,j;//5分个数，2分个数
    for(i=0,i<=20,i++);
    {
        for(j=0,j<=50,j++);
        {
            if((5*i+2*j)<=100);
            {
                count++
                printf("%d:5，%d:2,%d:1,%d\n,"count,i,j,100-5*i-2*j);
            }
        }
    }
}
