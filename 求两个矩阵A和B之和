#include <stdio.h>
main()
{    
     int A[3][4]={{1,2,3,4},{3,4,5,6},{5,6,7,8}};
     
     int B[3][4]={{1,2,3},{4,5},{6}};
     
     int i,j;
     
     for(i=0;i<3;i++)
     
        for(j=0;j<4;j++)
        
           A[i][j]+=B[i][j];
           
     for(i=0;i<3;i++)
      {   
         for(j=0;j<4;j++)
         
            printf("%6d",A[i][j]);
            
         printf("\n");
         
      }
}
