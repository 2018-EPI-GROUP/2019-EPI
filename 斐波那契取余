#include<stdio.h>
int main(void)
{
	int n, i , a[1000000];
    a[1] = 1 ;
    a[2] = 1 ;
    scanf("%d",&n);
    for( i = 3 ; i <= n ; i++ )
        {a[i] = ( a[i-1] + a[i-2] ) % 10007 ;}
	if(n <= 10007)
        {printf(" %d ", a[n] ); }
    else
        {printf(" %d " , a[n]%10007) ; }
    return 0;
}
