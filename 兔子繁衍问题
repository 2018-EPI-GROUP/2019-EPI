\\实质为斐波那契数列
#include<stdio.h>  
int main(void)
{      
	int N;      
	scanf("%d",&N);      
	if(N==1) printf("1");      
	else
	{          
		int i,m,n,sum,x;          
		sum=2,m=1,n=1,x=0;                    
		for(i=2;n<N;i++)
		{              
			x=m+n;              
			m=n;              
			n=x;      
		}      
		printf("%d",i);       
	}            
	return 0;  
}
