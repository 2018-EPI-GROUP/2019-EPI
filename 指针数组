#include<stdio.h>
main()
{
	int a[]={1,2,3};
	int b[]={4,5,6};
	int c[]={7,8,9};
	//指针数组是一个特殊的二维数组模型。 
	//指针数组相当于一个二级指针。 
	int* arr[]={a,b,c};
	
	int i,j;
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			//printf("%d  ",arr[i][j]);
			//printf("%d  ",*(arr[i]+j));
			printf("%d  ",*(*(arr+i)+j));
		}
		puts("");
	 } 
}
