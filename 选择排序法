#include<stdio.h>
main()
{
	int a[8];
	int i, j, t;
	for ( i = 0; i < 8; i++)
	{
		scanf_s("%d", &a[i]);
	}
	for ( i = 0; i < 8; i++)
	{
		for ( j = 0; j < 9-i; j++)
		{
			if (a[j] < a[j + 1])
			{
				t = a[j];
				a[j] = a[j+1];
				a[j + 1] = t;
			}
		}
	}
	for ( i = 0; i < 8; i++)
	{
		printf("%d\t", a[i]);
	}
}
