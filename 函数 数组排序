#include<stdio.h>
sh(int a[],int n)
{
	int i,j,t;
	for(i=0;i<9;i++)
	{
		for(j=0;j<9-i;j++)
		{
			if(a[j]>a[j+1])
			{
				t=a[j];
				a[j]=a[j+1];
				a[j+1]=t;
			}
		}
	}
	return a[10];
}



main()
{
	int a[10],b[10];
	int i;
	for(i=0;i<10;i++)
	{
		scanf("%d",&a[i]);
	}
	
	for(i=0;i<10;i++)
	{
		printf("%d ",a[i]);
	}
	
	sh(a,10);
	
	printf("\n");
	
	for(i=0;i<10;i++)
	{
		printf("%d ",a[i]);
	}
   }   
