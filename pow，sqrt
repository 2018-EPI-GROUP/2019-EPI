//pow（ x，y）的意思是x的y次方 
#include<stdio.h>
#include<math.h>

void main()
{
	double x=2.0 ,y=3.0, z ;
	z=pow(x,y);
	printf("%f//",z);
} 
 
//一般形式 
#include<stdio.h>

double pow(double x,double y); 
void main(void)
{
	double x=2.0 ,y=3.0, z ;
	z = pow(x,y);
	printf("%f",z);
} 
 
double pow(double x,double y)
{
	double z=1;
	while(y){
		z*=x;
		--y;
    }
    
    return z;
} 

//sqrt（x)的意思是根号下x
#include<stdio.h>
#include<math.h>
#include<stdlib.h> 

void main()
{
	double x=4.0, z ;
	z=sqrt(x);
	printf("%f",z);
} 

//一般形式
#include<stdio.h>

double sqrt(double x);
void main()
{
	double x, z ;
	scanf("%lf",&x);
	z=sqrt(x);
	printf("%f",z);
} 
 
double sqrt(double x)
{
	if(x==0)
	return 0;
	double last =0.0;
	double res =1.0;
	while(res != last){
		last = res;
		res = (res + x/res)/2;
	}
	return res;
}
