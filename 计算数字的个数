//计算1~10范围内的数字各有多少
#include<stdio.h>

int main()
{
	const int num=10;
	int count[num];
	int i;
	for(i=0;i<num;i++){
		count[i]=0;
	}
	int x;
	scanf("%d",&x);
	while(x!=-1){
		if(x>0&&x<10){
			count[x]++;
		}
		scanf("%d",&x);
	}
	for(i=1;i<num;i++){
	printf("%d有%d个\n",i,count[i]);
    }
	return 0;
} 
