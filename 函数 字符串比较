#include<stdio.h>
#include<string.h>

int my_strcmp(char ch1[],char ch2[])
	 {
	 	int i = 0;
	 	while(ch1[i] == ch2[i])
	 	{
	 		//是否到字符串结尾
			 if(ch1[i] =='\0')
			 {
			 	return 0;
			 }
		 
		 i++;
	    }
	 
	 return ch1[i]>ch2[i] ? 1:-1;
}
	 
int main()
{
	//比较两个字符串，相同返回0 不相同返回1或-1
	char ch1[]="hello";
	char ch2[]="hello";
	
	int value = my_strcmp(ch1,ch2);
	if(value == 0)
	{
		printf("两个字符串相同");
	} 
	else
	{
		printf("两个字符串不同");
	} 
	return 0;
}
