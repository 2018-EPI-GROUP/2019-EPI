#include <stdio.h>
int main()
{
	int a;
	printf("请输入您的成绩：");
	scanf("%d",&a);
	printf("您的评级为：");
	int n=a/10;
	switch(n)
   {    case 10:
		case 9:
		printf("A");
		break;
		case 8:
		printf("B");
		break;
		case 7:
		printf("C");
		break;
		case 6:
		printf("D");
		break;
		default:
		printf("E");
		break;
	 } 
	 return 0;
 } 
