#include<stdio.h>
int main()
{
	int i, s, count = 0, flag = 0;
	double c = 0, average = 0;
	for (i = 1;; i++)
	{
		scanf_s("%d", &s);
		if (s < 0) { break; }
		c++;
		average += s;
		if (s < 60 && s >= 0) {
			count++;
		}
	}
	if (i != 1) {
		printf("Average = %.2lf\n", average / c);
		printf("Count = %d", count);
	}
	else printf("Average = 0.00");
}
