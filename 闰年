#include<stdio.h>
 int main()
 {int leap,year;
  printf("请输入年份\n");
  scanf("%d",&year);
  if(year%400==0)
     {printf("%d是闰年.",year);}
  else if(year%4==0) 
         {if(year%100==0)
             {printf("%d不是闰年.",year);}
         else{printf("%d是闰年.",year);}  
         }  
       else{printf("%d不是闰年.",year);}  
   return 0;
}
