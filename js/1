#include<stdio.h>

#define SIZE 10

int main21()

{

	int arr[SIZE];

	for (int i = 0; i < SIZE; i++)

	{

		scanf_s("%d", &arr[i]);

		}

	int max = 0;

	for (int i = 0; i < SIZE; i++)

	{

		if (arr[i] > max)

		{

			max = arr[i];

		}

	}

	printf("最重的小猪体重为:%d\n", max);

	return 0;



}

#include<stdio.h>

#define SIZE 10

int main()

{

	int arr[SIZE];

	for (int i = 0; i < SIZE; i++)

	{

		scanf_s("%d", &arr[i]);

	}

	int max = arr[0];

	for (int i = 0; i < SIZE; i++)

	{

		if (arr[i] > max)

		{

			max = arr[i];

		}



	}

			printf("%d\n", max);

		

		return 0;
