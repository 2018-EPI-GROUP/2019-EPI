#include<stdio.h>
int main()
{
		int i,j,k;
	int arr[2][3][4]=
	{                             //大括号一个套一个，注意分号
		{
		{1,2,3,4},
		{2,3,4,5},
		{3,4,5,6}
	    },
	    {
	    	{4,5,6,7},
			{5,6,7,8},
			{6,7,8,9}
		}
	};
	for(i=0;i<2;i++)
	{
		for(j=0;j<3;j++)
		{
			for(k=0;k<4;k++)
			{
			printf("%d\t",arr[i][j][k]);        //规范操作，不要乱省略东西
		    }
		    	printf("\n");
		}	
		printf("\n");
	}	
 } 
