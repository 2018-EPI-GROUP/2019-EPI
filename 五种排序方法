//快速排序
void *swap(int *x, int *y);
int qusort(int p[], int start, int end);
int main()
{
	int a[11]={55,48,23,96,59,12,02,44,69,36,37};
	qusort(a,0,10);	
	for(int i = 0; i < 11; i ++) 
		printf("%d ",a[i]);
}

int qusort(int p[], int start, int end)
{
	int x = p[start];
	int i = start, j = end;
	while(j > i){
		while(x < p[j]) j--;
		swap(&p[i], &p[j]);
		while(x > p[i])	i++;
		swap(&p[i], &p[j]);	
	}
	if(start < i)
		qusort(p, start, i-1);
	if(i < end)
		qusort(p, j+1, end);
}

void *swap(int *x, int *y)
{
	int temp = *x;
	*x = *y;
	*y = temp;
}
