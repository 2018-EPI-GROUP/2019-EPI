#include<stdio.h>
#define N 99 
int main()
{
	int j, i,t ,n;
	int arr[N+1];
    scanf("%d",&n);
    printf("input 'n' number\n");
    for(i=0;i<n;i++)
    scanf("%d",&arr[i]);
    
	for ( i = 0; i < n-1; i++)
	{
		for(j=0;j<n-1-i;j++)
		{
			if(arr[j]>arr[j+1])
			{
		    t=arr[j];
		    arr[j]=arr[j+1];
		    arr[j+1]=t;
	        }     
	
	    }  

    }
        for(i=0;i<n;i++)
	    printf("%d\n", arr[i]);
}
