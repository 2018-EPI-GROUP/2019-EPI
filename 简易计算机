//简易计算机，可以运算+，-，*，/，% 
#include<stdio.h>
int main()
{
	int a,b;
	double c; 
	char d;
	scanf("%d %c %d",&a,&d,&b);
	switch(d)
	{
		case '+':
			c=a+b;
			break;
		case '-':
			c=a-b;
			break;
		case '*':
			c=a*b;
			break;
		case '/':
			c=a/b;
	    	break;
	    case '%':
	    	c=a%b;
	    	break;
	    default:
	    	printf("ERROR");
	    	break;
	}
	printf("%f",c);
	return 0;
} 
