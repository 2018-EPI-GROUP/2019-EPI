//递归应用，将问题分解为更小的问题
#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<iostream>
using namespace std;

int f(int n);

int main()
{
	int n;

	cin >> n;

	cout << f(n)<< endl;

	system("pause");

	return 0;
}

int f(int n)
{
	if ( n == 1 ) 
		return 1;
	
	else if (n == 2)				//给出边界值，防止无穷递归
		return 2;
	
	else if (n == 3) 
		return 3;
	
	return f(n - 1) + f(n - 2);
}
