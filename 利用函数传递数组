#include<stdio.h>
#define MAX 10
int largest(int num_array[],int length);
int array[MAX],count;
int main (void)
{
    for(count = 0;count < MAX; count++)
	 scanf("%d",&array[count];
  printf("\n larget value = %d",largest(array,length));
}
int largest (int num_array[],int length)
{
    int count ,biggest;

    for (count = 0;count< length; count++)
    {
	 if(count ==0)
	     biggest = num_array[count];
	 if(num_array[count] > biggest)
	     biggest = num_array[count];
    }
    return biggest;
}

