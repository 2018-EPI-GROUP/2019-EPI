#include <stdio.h>
#include<math.h> 
int main()
{
	float x,y;
	scanf("%f",&x);       //平时算函数时要记得 float啊，嘿嘿嘿
	if(x<0)
	y=0;
	else if(x<20)
	y=5*x+1;
	else if(x<40)
	y=sin(x/180*3.14)+cos(x/180*3.14);
	else if(x<60)
	y=exp(x)-1;               //自然对数底‘e’的’x‘的次方的计算就是用函数  exp（x） 
	else if(x<80)
	y=log(x);             //ln（x）用C语言计算就是  log（x），x的y次方计算就是  pow（x，y） 
	else y=0;
	printf("%f",y);
}
