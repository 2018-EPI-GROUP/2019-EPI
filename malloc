#include<stdio.h>
#include<stdlib.h>
int main()
{
	void check(int* );
	int *p1,i;
	p1=(int *)malloc(5*sizeof(int));  //开辟动态存储去，将地址转换成int*行，然后放在p1中
	for(i=0;i<5;i++)
		scanf("%d",p1+i);
	check(p1);
	return 0;
}

void check(int*p)
{
	int i;
	printf("They are fail:");
    for(i=0;i<5;i++)
			if(p[i]<60)printf("%d",p[i]);
			printf("\n");
}
