#include<stdlib.h>
#include<time.h> 
#include<stdio.h>
int like( char ch1[],char ch2[])
{
	int i=0;
	while(ch1[i]==ch2[i])
	{
		if(ch1[i]=='\0')
		return 0;
		i++;
	}
	return 1;
	
 } 
 int main()
 {
 char ch1[]="hello";
 char ch2[]="hello";
int value = like(ch1,ch2);
if(value==0)
 { printf("一样");}
else
 {printf("不一样");}
}
 
