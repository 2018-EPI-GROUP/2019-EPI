//三阶矩阵相乘
#include<stdio.h>
int main()
{
	int a,b;   
	int m,c;
	int x[3][3],y[3][3],z[3][3];
	printf("请输入第一个矩阵\n");
	for(a=0;a<3;a++){
		for(b=0;b<3;b++){
			scanf("%d",&m);
			x[a][b]=m;
		}
	} 
	printf("请输入第二个矩阵\n");
	for(a=0;a<3;a++){
		for(b=0;b<3;b++){
			scanf("%d",&m);
			y[a][b]=m;
		}
	} 
	for(a=0;a<3;a++){
		for(b=0;b<3;b++){
		    for(c=0;c<3;c++){
		    	z[a][b]=x[a][c]*y[c][b];
			}
		}
	} 
	for(a=0;a<3;a++){
		for(b=0;b<3;b++){
			printf("%3d",z[a][b]);
		}
		printf("\n");
	} 
} 
