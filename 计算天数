#include<stdio.h>
struct Date
{
	int month;
	int day;
	int year;
};
int main()
{
    int a[12]={31,28,31,30,31,30,31,31,30,31,30,31};
	int sum,i;
	struct Date p;
	scanf("%d%d%d",&p.year,&p.month,&p.day);
	sum=p.day;
	for(i==0,i<p.month-1;i++;)
	{
		sum=sum+a[i];
	}
	
	if(((p.year%4==0&&p.year%100!=0)||p.year%400==0)&&p.month>2)
	{
		printf("%d",sum+1);
	}
		else if((p.year%4!=0))
			printf("%d",sum);
	
		return 0;
	
}
