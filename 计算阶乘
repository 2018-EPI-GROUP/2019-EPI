#include <stdio.h>

void Print_Factorial ( const int N );

int main()
{
    int N;
	
    scanf ("%d",&N);
    Print_Factorial(N);
    return 0;
}
void Print_Factorial ( const int N )
{   int T=N;
	if (N<0)
		printf ("Invalid input");
	else
	{	for (int i=1;i<N;i++)
	    	
         T =T*i;
		
		printf ("%d\n",T);
    }
}
