{
	void copy(char *from,char *to);
	
	char *a = "good afternoon!";
	char b[] = "good morning!";
	printf("%s,%s\n",a,b);	
	copy(a,b);
	
	printf("%s,%s\n",a,b);
}

void copy(char *from,char *to)
{
	while((*to++ = *from++) != '\0'){
		;
	}
	*to = '\0';
}

